if flash[:error]
  page.call 'alert', flash[:error]
else
  if @feed.active?
    page["feed_#{@feed.id}"].remove_class_name 'inactive_feed'
  else
    page["feed_#{@feed.id}"].add_class_name 'inactive_feed'
  end
end