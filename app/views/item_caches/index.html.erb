<div class="control_bar">
<%= link_to 'New Item Cache', new_item_cache_path %>
</div>

<table class="data_table">
  <tr>
      <th>Cache URI</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
  </tr>

<% for item_cache in @item_caches %>
  <tr class="<%= cycle('odd', 'even') %>">
    <td><%= link_to item_cache.base_uri, item_cache.base_uri %></td>
    <td><%= link_to 'Edit', edit_item_cache_path(item_cache) %></td>
    <td><%= link_to 'Destroy', item_cache, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

